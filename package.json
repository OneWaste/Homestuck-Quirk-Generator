{
  "name": "homestuck-quirk-generator",
  "version": "4.0.0",
  "description": "A site that allows you to convert inputted text into the typing quirk of a Homestuck character.",
  "main": "Src/index.ts",
  "scripts": {
    "clean": "rimraf ./deployment/*",
    "frontend-build": "./node_modules/.bin/vue-cli-service serve --no-clean --dest ./deployment",
    "frontend-watch": "yarn run frontend-build --watch",
    "local-serve": "yarn run clean && yarn run frontend-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://QuaternionMark@github.com/QuaternionMark/Homestuck-Quirk-Generator.git"
  },
  "author": "Mark Jarjour",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/QuaternionMark/Homestuck-Quirk-Generator/issues"
  },
  "homepage": "https://quaternionmark.github.io/homestuck_quirk/",
  "dependencies": {
    "core-js": "^3.6.5",
    "rimraf": "^3.0.2",
    "vue": "^3.0.0",
    "vue-class-component": "^8.0.0-0",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "concurrently": "^5.3.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "typescript": "~4.1.2"
  }
}
